<style>
    
</style>
<template>
	<div>
		<div class="input">
            <Input placeholder="Search" clearable icon="ios-search-strong"></Input>
        </div>
        <div class="tree" >
            <Tree :data="data1"></Tree>
        </div>
	</div>
</template>
<script>
	export default {
		name: 'tree',
        data () {
            return {
                data1: []
            }
        },
        methods: {
        	getTrees: function() {
                let vm = this;
                $.ajax({
                    url: '../../src/data/tree.json',
                    dataType: "json",
                    success: function(data) {
                        vm.data1 = data;
                    },
                    error: function() {
                        console.log('fail');
                    }
                })
            },
        },
        mounted: function() {
            this.getTrees();
        }
    }
</script>